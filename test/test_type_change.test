daemon d PEER
clean local
clean peer
FILENAME="new_file 'N' all"
DIRNAME=type_change
mkdir -p "test/$NAME/$DIRNAME"
echo "far" > "test/$NAME/$DIRNAME/$FILENAME"
cmd x "New $NAME/$DIRNAME/$FILENAME"
cmd T test/$NAME "List daemon" 
rm -rf test/$NAME/$DIRNAME
sleep 2
echo "now file" > "test/$NAME/$DIRNAME"
cmd x "Directory to file (type change)"
#
rm  test/$NAME/$DIRNAME
sleep 1
mkdir -p "test/$NAME/$DIRNAME"
echo "far" > "test/$NAME/$DIRNAME/$FILENAME"
cmd x "File 2 directory (type change)"
#
rm -rf test/$NAME/$DIRNAME
sleep 2
echo "file again" > "test/$NAME/$DIRNAME"
cmd x "Directory to file (type change)"
#
rm -rf test/$NAME
cmd x "clean up"
killdaemon d PEER
diff -r /tmp/csync2.real/ test_type_change/backup.res/