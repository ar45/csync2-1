daemon d PEER
clean local
clean peer
rm -rf test/local
cmd x "clean"
mkdir -p test/local/auto/older
cmd c "3 create directories (check)"
cmd u "4 create directories (update)"
sleep 1
echo "peer older" > test/peer/auto/older/update_both
sleep 1
echo "local younger" > test/local/auto/older/update_both
cmd c "5 Check local (autoresolve failed)"
cmd u "6 Fail to autoresolve (not older)"
sleep 1
echo "Now older" > test/peer/auto/older/update_both
cmd c "7 Check local (older)"
cmd u "9 Update auto (older success)"
rm -rf test/local/older
cmd x "9 clean (older)"
killdaemon d PEER