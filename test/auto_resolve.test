daemon d PEER
clean local
clean peer
rm -rf test/local
cmd x "clean"
mkdir -p test/local/auto/younger
mkdir -p test/local/auto/older
mkdir -p test/local/auto/larger
mkdir -p test/local/auto/smaller
cmd c "1 create directories (check)"
cmd u "2 create directories (update)"
echo "local new" > test/local/auto/younger/update_both
sleep 1
echo "peer younger" > test/peer/auto/younger/update_both
cmd c "3 Check local (autoresolve failed)"
cmd u "5 Fail to autoresolve (not yonger)"
sleep 1
echo "Now younger" > test/local/auto/younger/update_both
cmd c "3 Check local (autoresolve)"
cmd u "4 Update auto (younger)"
killdaemon d PEER